import java.util.*;
public class HelloWorld{
    static int multiply(int num1, int num2){
        return num1*num2;
    }
    public static void main(String[]args){
      Scanner sc =new Scanner(System.in);
      int num1=sc.nextInt();
      int num2=sc.nextInt();
      int mul=multiply(num1,num2);
      System.out.println(num1+"*"+num2+"="+mul);
    }
}